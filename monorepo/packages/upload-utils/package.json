{
  "name": "@thulobazaar/upload-utils",
  "version": "1.0.0",
  "description": "Platform-agnostic upload utilities for web and React Native",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./web": {
      "types": "./dist/web.d.ts",
      "import": "./dist/web.mjs",
      "require": "./dist/web.js"
    },
    "./native": {
      "types": "./dist/native.d.ts",
      "import": "./dist/native.mjs",
      "require": "./dist/native.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@thulobazaar/types": "*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "expo-image-picker": ">=14.0.0",
    "expo-image-manipulator": ">=11.0.0"
  },
  "peerDependenciesMeta": {
    "expo-image-picker": {
      "optional": true
    },
    "expo-image-manipulator": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "upload",
    "image",
    "file",
    "react-native",
    "expo",
    "cross-platform"
  ]
}
